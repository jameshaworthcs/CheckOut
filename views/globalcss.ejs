<style>
    /* Select the table element */
    #data-table {
      /* Add a border around the table */
      border: 1px solid rgb(68, 68, 68);
      /* Add some space between the table and other elements */
      margin: 15px;
    }
  
    /* Select the table cells */
    #data-table td {
      /* Add a border around each cell */
      border: 1px solid rgb(68, 68, 68);
      /* Add some padding inside each cell */
      padding: 5px;
    }
    body {
      margin: 0;
      font-size: 20px;
      font-family: Arial, Helvetica, sans-serif;
      background-color:white;
    }
    
    .header {
      background-color: #f1f1f1;
      padding: 30px;
      text-align: center;
    }
    
    #navbar {
      overflow: hidden;
      background-color: transparent;
    }
    /* #38444d is old background for nav */
    
    #navbar a {
      float: left;
      display: block;
      color: #f2f2f2;
      text-align: center;
      padding: 14px 16px;
      text-decoration: none;
      font-size: 17px;
      position: relative;
    }
    #navbar a:not(:last-child)::after {
      content: '';
      position: absolute;
      height: 70%;
      width: 1.2px;
      background-color: #ccc; /* Vertical line color */
      top: 15%; /* Adjust the position of the line */
      right: 0;
    }
    
    #navbar a:hover {
      background-color: #ddd;
      color: black;
    }
    
    #navbar a.active {
      background-color: #04AA6D;
      color: white;
    }
    
    
    .content {
      padding: 16px;
    }
    
    .sticky {
      position: fixed;
      top: 0;
      width: 100%;
    }
    
    .sticky + .content {
      padding-top: 60px;
    }
    .btn-link {
        border: none;
        outline: none;
        background: none;
        cursor: pointer;
        float: left;
        display: block;
        color: #f2f2f2;
        text-align: center;
        padding: 14px 16px;
        text-decoration: none;
        font-size: 17px;
    }
    body {
          font-family: 'Arial', sans-serif;
          background-color: var(--bg-color);
          color: var(--text-color);
          transition: background-color 0.5s, color 0.5s;
        }
    
        form {
          max-width: 400px;
          margin: 0 auto;
          padding: 20px;
          background-color: var(--form-bg-color);
          border-radius: 8px;
          box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
          transition: background-color 0.5s;
        }
    
        label {
          display: block;
          margin-bottom: 8px;
        }
    
        select,
        input[type="text"], input[type="tel"] {
          width: 100%;
          padding: 8px;
          margin-bottom: 12px;
          box-sizing: border-box;
          background-color: var(--input-bg-color);
          color: var(--input-text-color);
          border: 2px solid var(--input-border-color);
          border-radius: 4px;
          font-size: 20px !important;
        }
    
        input[type="text"]:focus,
        input[type="tel"]:focus,
        select:focus {
          outline: none;
          border-color: var(--input-focus-border-color);
        }
    
        table {
          margin-top: 20px;
          margin-left: 15px;
          border-collapse: collapse;
        }
    
        th, td {
          padding: 10px;
          border: 1px solid rgb(68, 68, 68);
          text-align: left;
        }
        tr:first-child td:last-child {
            border-top: none;
            border-right: none;
        }
    
        input[type="submit"] {
            background-color: white !important;
            color: black !important;
          padding: 10px;
          border: none;
          border-radius: 4px;
          cursor: pointer;
          transition: background-color 0.3s;
        }
    
        input[type="submit"]:hover {
          background-color: var(--submit-btn-hover-bg-color);
        }
        select {
            background-color: rgb(68, 68, 68);
        }
        input[type="text"], input[type="tel"] {
            background-color: rgb(68, 68, 68);
        }
        .homecontent {
            padding: 15px;
        }
        .sub-table-info {
            padding: 15px;
        }
        .sub-table-link, .sub-table-link:visited {
            color: #58a6ff;
            text-decoration: none;
        }
        .sub-table-link:hover {
            color: rgb(43, 255, 85);
            text-decoration: underline;
        }
        .copy-confirmation {
        position: fixed;
        top: 0;
        left: 50%;
        transform: translateX(-50%);
        padding: 10px;
        background-color: #4CAF50;
        color: white;
        font-weight: bold;
        text-align: center;
        width: 100%;
        box-sizing: border-box;
        z-index: 999;
        opacity: 0;
        transition: opacity 0.5s ease;
        pointer-events: none; /* Allow interactions with elements beneath */
    }

    .copy-confirmation.show {
        opacity: 1;
        pointer-events: auto; /* Enable interactions when the confirmation is visible */
    }
    .share-button {
      cursor: pointer;
      padding: 5px 13px;
      background-color: #222;
      color: #fff;
      border: solid;
      border-radius: 4px;
    }
    /* Add some basic styling to the link */
    #fixed-link {
            position: fixed;
            bottom: 20px;
            font-size: 15px;
            right: 20px;
            background-color: #0000003d;
            color: #58a6ff;
            padding: 10px;
            text-decoration: none;
            border-radius: 1px;
            display: none;
        }
        @media (prefers-color-scheme: dark) {
          body {
            background-color: #0D1117;
            color: #afafaf;
          }
          input[type="submit"] {
            background-color: var(--submit-btn-bg-color);
          color: white;
          }
          .verified-repeatOLD {
            filter: invert(1) sepia(1) saturate(20) hue-rotate(500deg) brightness(0.8);
            }
        }
        </style>