lement('div'); errorBox.className = 'error-box'; const svgIcon = `<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24" class="icon-sm"><path fill="currentColor" d="M12.853 5.314a1 1 0 0 0-1.706 0L3.713 17.479A1 1 0 0 0 4.566 19h14.868a1 1 0 0 0 .853-1.521zM9.44 4.271c1.17-1.914 3.95-1.914 5.12 0l7.434 12.165c1.221 1.999-.217 4.564-2.56 4.564H4.566c-2.343 0-3.781-2.565-2.56-4.564zM12 9a1 1 0 0 1 1 1v3a1 1 0 1 1-2 0v-3a1 1 0 0 1 1-1m-1.148 7.5a1.15 1.15 0 1 1 2.3 0 1.15 1.15 0 0 1-2.3 0"></path></svg>`; errorBox.innerHTML = svgIcon + '<p>'+message+'</p>'; targetDiv.innerHTML = ''; // Clear the existing content targetDiv.appendChild(errorBox); // Show error on submit form document.querySelector('.submit-skeleton').classList.add('hidden'); document.querySelector('.submit-form').classList.remove('hidden'); // Show error on classes content const classesSkeleton = document.querySelector('.classes-skeleton'); const classesContent = document.querySelector('.classes-content'); classesSkeleton.style.display = 'none'; classesContent.style.display = 'block'; }/////////////////////// Start API calls ///////////////////////function setup() { getV2().then(data => { const { sessionCount, msg, userPerms, sessions } = data; fillSubmitForm(sessionCount, msg, sessions); fillCodesDisplay(sessionCount, msg, sessions); }).catch(error => { console.error(error); // Handle error and display error message if (error.response && error.response.msg) { showErrorBox(error.response.msg, 'submit-form'); showErrorBox(error.response.msg, 'classes-content'); } else { showErrorBox("Something went wrong. Unable to connect to API.", 'submit-form'); showErrorBox("Something went wrong. Unable to connect to API.", 'classes-content'); } });}document.addEventListener('DOMContentLoaded', function () { setup();});////////////////// GET v2 API //////////////////async function getV2() { try { const response = await fetch('/api/app/v2'); if (!response.ok) { const errorResponse = await response.json(); throw { status: response.status, response: errorResponse }; } const jsonResponse = await response.json(); const { sessionCount, msg, userPerms, sessions } = jsonResponse; return { sessionCount, msg, userPerms, sessions }; } catch (error) { if (error.response) { console.error('Error response from API:', error.response); throw error; } else { console.error('Error fetching API:', error); throw new Error('Failed to fetch data from API'); } }}//////////////////////// Fill submit form ////////////////////////function fillSubmitForm(sessionCount, msg, sessions) { const submitFormElement = document.getElementById('submit-form'); let sessionHtml; if (sessionCount == 0) { sessionHtml = `No classes are in progress, come back later.`; } else { sessionHtml = sessions.map(session => { return ` <li> Start Date: ${session.startDate}, Start Time: ${session.startTime}, End Time: ${session.endTime}, Description: ${session.description}, Module Name: ${session.moduleName}, Module Code: ${session.moduleCode}, Reject ID: ${session.rejectID}, Location: ${session.location}, Codes Count: ${session.codesCount}, Codes: ${session.codes.map(code => `Group Code: ${code.groupCode}, Checkin Code: ${code.checkinCode}, Reject Score: ${code.rejectScore}, Count: ${code.count}` ).join(', ')} </li> `; }).join(''); sessionHtml = `<ul>${sessionHtml}</ul>` } submitFormElement.innerHTML = `${sessionHtml}`; document.querySelector('.submit-skeleton').classList.add('hidden'); document.querySelector('.submit-form').classList.remove('hidden');}////////////////////////// Fill codes display //////////////////////////function fillCodesDisplay(sessionCount, msg, sessions) { const codesDisplayElement = document.getElementById('classes-content'); if (sessionCount == 0) { codesDisplayElement.innerHTML = `No classes in session.`; } else { const codeInfo = generateCodeInfo(sessionCount, sessions); const classesHtml = sessions.map(session => { return ` <li> Start Date: ${session.startDate}, Start Time: ${session.startTime}, End Time: ${session.endTime}, Description: ${session.description}, Module Name: ${session.moduleName}, Module Code: ${session.moduleCode}, Reject ID: ${session.rejectID}, Location: ${session.location}, Codes Count: ${session.codesCount}, Codes: ${session.codes.map(code => `<li>Group Code: ${code.groupCode}, Checkin Code: ${code.checkinCode}, Reject Score: ${code.rejectScore}, Count: ${code.count}</li>` ).join(', ')} </li> `; }).join(''); codesDisplayElement.innerHTML = `${codeInfo}<ul>${classesHtml}</ul>`; } const classesSkeleton = document.querySelector('.classes-skeleton'); const classesContent = document.querySelector('.classes-content'); classesSkeleton.style.display = 'none'; classesContent.style.display = 'block';}function generateCodeInfo(sessionCount, sessions) { if (sessionCount >= 5) return ''; const codesPerSession = sessions.map(session => session.codesCount); let codeInfo = ''; if (sessionCount === 1) { if (codesPerSession[0] === 0) { codeInfo = 'No codes are available for the class in progress'; } else if (codesPerSession[0] === 1) { codeInfo = 'A code is available for the class in progress'; } else if (codesPerSession[0] > 1) { codeInfo = 'Multiple codes are available for the class in progress'; } } else if (sessionCount === 2) { if (codesPerSession[0] === 0 && codesPerSession[1] === 0) { codeInfo = 'No codes are available for either of the two classes in progress'; } else if (codesPerSession[0] === 1 && codesPerSession[1] === 1) { codeInfo = 'A code is available for each of the two classes in progress'; } else if (codesPerSession[0] === 1 && codesPerSession[1] > 1) { codeInfo = `A code is available for one of the two classes in progress and ${codesPerSession[1]} codes are available for the other`; } else if (codesPerSession[0] > 1 && codesPerSession[1] === 1) { codeInfo = `${codesPerSession[0]} codes are available for one of the two classes in progress and a code is available for the other`; } else if (codesPerSession[0] > 1 && codesPerSession[1] > 1) { codeInfo = `${codesPerSession[0]} codes are available for one of the two classes in progress and ${codesPerSession[1]} codes are available for the other`; } else if (codesPerSession[0] === 1 || codesPerSession[1] === 1) { codeInfo = 'A code is available for one of the two classes in progress'; } else if (codesPerSession[0] > 1 || codesPerSession[1] > 1) { codeInfo = `Multiple codes are available for one of the two classes in progress`; } } else if (sessionCount === 3) { if (codesPerSession.every(count => count === 0)) { codeInfo = 'No codes are available for the three classes in progress'; } else { const messages = sessions.map((session, index) => { const count = codesPerSession[index]; if (count === 0) return `${(index==0)?'N':'n'}o codes available`; if (count === 1) return `${(index==0)?'O':'o'}ne code available`; return `${count} codes available`; }); codeInfo = messages.join(', ').replace(/, ([^,]*)$/, ' and $1 for the classes in progress'); } } else if (sessionCount === 4) { if (codesPerSession.every(count => count === 0)) { codeInfo = 'No codes are available for the four classes in progress'; } else { const messages = sessions.map((session, index) => { const count = codesPerSession[index]; if (count === 0) return `${(index==0)?'N':'n'}o codes available`; if (count === 1) return `${(index==0)?'O':'o'}ne code available`; return `${count} codes available`; }); codeInfo = messages.join(', ').replace(/, ([^,]*)$/, ' and $1 for the classes in progress'); } } codeInfo += '.'; return codeInfo;}</script> </body></html>