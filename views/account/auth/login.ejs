<%
let contentTemplate;
switch(loginMethod) {
    case 'google':
        contentTemplate = 'google';
        break;
    case 'email':
        contentTemplate = 'email';
        break;
    case 'apikey':
        contentTemplate = 'apikey';
        break;
    case 'selection':
        contentTemplate = 'selection';
        break;
    default:
        contentTemplate = 'google';
}
%>
<%- include('./login-methods/base', { contentTemplate, msg, guestState }); %>