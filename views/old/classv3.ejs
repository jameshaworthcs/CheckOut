function fetchModulesAndPopulateNav() {
    // Check if all values are present in local storage
    var institutionID = localStorage.getItem('inst');
    var year = localStorage.getItem('yr');
    var course = localStorage.getItem('crs');

    // Check if all values are present
    if (institutionID && year && course) {
        // Make API request
        var apiUrl = `https://<%- rootDomain %>/api/app/find/${institutionID}/${year}/${course}/md`;
        fetch(apiUrl)
            .then(response => response.json())
            .then(data => {
                // Clear existing items from the navbar
                var navbar = document.querySelector('.navbar-nav');
                navbar.innerHTML = '';

                // Create new list items for each module
                data.modules.forEach(module => {
                    var listItem = document.createElement('li');
                    listItem.classList.add('nav-item');

                    var link = document.createElement('a');
                    link.classList.add('page-scroll');
                    link.href = `https://<%- rootDomain %>/api/app/class/${institutionID}/${course}/${year}/${module.module_code}`;
                    link.textContent = module.module_name;

                    listItem.appendChild(link);
                    navbar.appendChild(listItem);
                });
                // Clear existing items from the sidebar links
                var sidebarLinks = document.querySelector('.sidebarLinks');
                sidebarLinks.innerHTML = '';
                var sidebarLinks2 = document.querySelector('.sidebarLinks2');
                sidebarLinks2.innerHTML = '';

                // Create new list items for each module
                data.modules.forEach(module => {
                    var listItem = document.createElement('li');

                    var link = document.createElement('a');
                    link.href = `https://<%- rootDomain %>/api/app/class/${institutionID}/${course}/${year}/${module.module_code}`;
                    link.textContent = module.module_name;

                    listItem.appendChild(link);
                    sidebarLinks.appendChild(listItem);
                });
                // Clear existing items from the sidebar links
                var footerModules = document.querySelector('.footerModules');
                footerModules.innerHTML = '';

                // Create new list items for each module
                data.modules.forEach(module => {
                    var listItem = document.createElement('li');

                    var link = document.createElement('a');
                    link.href = `https://<%- rootDomain %>/api/app/class/${institutionID}/${course}/${year}/${module.module_code}`;
                    link.textContent = module.module_name;

                    listItem.appendChild(link);
                    footerModules.appendChild(listItem);
                });

                // Append additional links regardless
                appendAdditionalSidebarLinks(sidebarLinks2);
            })
            .catch(error => console.error('Error fetching modules:', error));
    } else {
        console.error('Not all values are present in local storage');
    }
}
// Check if all required values are present in local storage
var institutionId = localStorage.getItem('inst');
var year = localStorage.getItem('yr');
var course = localStorage.getItem('crs');

if (institutionId && year && course) {
    // All values are present, construct the URL for the iframe
    var iframeUrl = 'https://<%- rootDomain %>/api/app/classframe/' + institutionId + '/' + course + '/' + year + '/<%- md %>';

    // Create the iframe element
    var iframe = $('<iframe>', {
        id: 'myIframe',
        src: iframeUrl,
        style: 'border:0px #000000 none; max-width:400px;',
        name: 'myiFrame',
        scrolling: 'no',
        frameborder: 0,
        marginheight: '0px',
        marginwidth: '0px',
        width: '100%',
        allowfullscreen: true
    });

    // Append the iframe to the container
    $('#iframeContainer').append(iframe);
    fetchModulesAndPopulateNav()
    resizeIframe()
} else {
    // Redirect the page to onboarding
    window.location.href = '/api/app/onboarding';
}

function resizeIframe() {
    var iframe = document.getElementById("myIframe");
    var container = document.getElementById("iframeContainer");
    if (iframe && container) {
        iframe.style.height = (iframe.contentWindow.document.body.scrollHeight + 1) + "px";
    }
}

// Call resizeIframe initially and whenever the window resizes
window.onload = resizeIframe;
window.onresize = resizeIframe;

// Observe mutations within the iframe content
var observer = new MutationObserver(resizeIframe);
observer.observe(document.getElementById('myIframe').contentWindow.document.body, {
    childList: true,
    subtree: true,
    attributes: true
});
