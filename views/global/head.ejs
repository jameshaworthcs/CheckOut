    /* Adds external link indicator */
    a.external {
        background-image: url(/static/images/external-icon.svg);
        background-position: center right;
        background-repeat: no-repeat;
        background-size: 0.857em;
        padding-right: 1em;
    }

    /*** Works on common browsers ***/
    ::selection {
        background-color: #32443d;
        color: var(--text-colour);
    }

    /*** Mozilla based browsers ***/
    ::-moz-selection {
        background-color: #32443d;
        color: var(--text-colour);
    }

    /***For Other Browsers ***/
    ::-o-selection {
        background-color: #32443d;
        color: var(--text-colour);
    }

    ::-ms-selection {
        background-color: #32443d;
        color: var(--text-colour);
    }

    /*** For Webkit ***/
    ::-webkit-selection {
        background-color: #32443d;
        color: var(--text-colour);
    }

    /* Wrapper to extend the border */
    .navbar-wrapper {
        width: 100%;
        border-bottom: 1px solid var(--navbar-border-colour);
        background-color: var(--navbar-background-colour);
        background: var(--navbar-background);
        background-size: auto 670%;
        box-shadow: var(--shadow);
    }

    /* Modify the navbar to remove the border */
    .navbar {
        font-size: 14px;
        font-weight: 600;
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 0 5%;
        max-width: 1200px;
        margin: 0 auto;
        border-bottom: none; /* Remove the border from here */
        background: var(--navbar-background);
    }


    .navbar a:not(.cta-button) {
        color: var(--header-colour);
        text-decoration: none;
        margin: 0 10px;
    }

    .navbar a:hover {
        text-decoration: none;
        /* color: rgba(255, 255, 255, 0.725); */
        opacity: .8;
    }

    .nav-icon {
        height: 20px;
        fill: var(--header-colour);
        vertical-align: middle;
        margin-block-end: 3px;
        margin-left: 20px;
        margin-right: 4px;
    }

    .logo {
        background-image: var(--logo-image);
        background-repeat: no-repeat;
        background-position: center;
        background-size: 102px;
        width: 102px;
        background-position-y: 13px;
        height: 44px;
        padding-top: 5px;
        padding-bottom: 0;
        /* for ios */  
        border: none;           /* Remove border */
        color: transparent;     /* Hide alt text */
        font-size: 0;          /* Extra protection for alt text */
        margin-left: 10px;
    }

    .cta-tooltip {
    position: relative;
    display: inline-block;
    }

    .cta-tooltip .cta-tooltiptext {
        visibility: hidden;
        width: 320px;
        background-color: black;
        color: #fff;
        text-align: center;
        border-radius: 5px;
        padding: 5px 4px;
        position: absolute;
        z-index: 1;
        top: 150%;
        left: 50%;
        margin-left: -307px;
        margin-top: 17px;
        /* Add transition for fade in effect */
        opacity: 0;
        transition: opacity 0.3s ease-in;
    }

    /* When visible, set opacity to 1 */
    .cta-tooltip .cta-tooltiptext.visible {
        opacity: 1;
    }

    .cta-tooltip .cta-tooltiptext::after {
        content: "";
        position: absolute;
        bottom: 100%;
        left: 76%;
        margin-left: -5px;
        border-width: 5px;
        border-style: solid;
        border-color: transparent transparent black transparent;
    }

    #darkTooltipClose {
        color: var(--subtext-colour);
    }

    /* .cta-tooltip:hover .cta-tooltiptext {
        visibility: visible;
    } */

