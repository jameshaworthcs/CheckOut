        /* border: 1px solid #3dcbff; */
        border: var(--box-accent-width) solid var(--accent-colour);
        background: var(--content-background);
        padding: 24px;
        max-width: 500px;
        border-radius: 10px;
        margin-bottom: 20px;
        box-shadow: var(--shadow);
        transition: height 0.5s ease, max-height 0.5s ease;
        /* Add transition property */
        overflow: hidden;
        /* Ensure content doesn't overflow */
    }

    .subtext {
        color: var(--subtext-colour);
    }

    .submit-skeleton,
    .submit-form {
        transition: opacity 0.5s ease;
        /* Smooth transition for content visibility */
    }

    .submit-skeleton.hidden,
    .submit-form.hidden {
        display: none;
    }

    .submit-skeleton.hidden {
        height: 0;
        overflow: hidden;
    }

    .submit-form {
        height: auto;
        /* Ensure the form expands as needed */
    }

    .box h2 {
        font-size: 19px;
        margin-block-start: 0;
        margin-bottom: 10px;
        line-height: 22.8px;
        font-weight: 600;
    }

    .box p {
        font-size: 16px;
        font-weight: 400;
        letter-spacing: -0.48px;
        line-height: 24px;
        margin: 0;
    }

    .cta-button {
        display: inline-block;
        padding: 10px 20px;
        background-color: var(--cta-background);
        background-size: auto 270%;
        color: var(--cta-button-colour);
        text-decoration: none;
        border-radius: 7px;
        padding-inline-start: 0.75rem;
        padding-inline-end: 0.75rem;
        font-size: 0.87rem;
        font-weight: 550;
        cursor: pointer;
        border: var(--cta-border);
    }

    .cta-button-main {
        background-color: var(--cta-background-main);
        color: var(--cta-button-colour-main);
    }

    .cta-button:hover {
        background-color: var(--action-highlight-colour);
    }

    .cta-button-main:hover {
        opacity: 0.9;
        background-color: var(--action-highlight-colour-main);
    }

    .menu-icon {
        display: inline-flex;
        width: 18px;
        height: 18px;
        cursor: pointer;
        vertical-align: middle;
        transform: translateY(-2px);
        margin-right: 6.5px;
