        padding: 20px;
        background: var(--secondary-background);
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
        border-radius: 8px;
    }

    .skeleton {
        background: linear-gradient(90deg, 
            var(--skeleton-gradient-start) 25%, 
            var(--skeleton-gradient-middle) 50%, 
            var(--skeleton-gradient-end) 75%
        );
        background-size: 200% 100%;
        animation: loading 1.5s infinite;
        border-radius: 4px;
        margin-bottom: 10px;
    }

    .skeleton-name {
        background: linear-gradient(90deg, 
            var(--skeleton-name-gradient-start) 25%, 
            var(--skeleton-name-gradient-middle) 50%, 
            var(--skeleton-name-gradient-end) 75%
        );
        background-size: 200% 100%;
        animation: loading 1.5s infinite;
        border-radius: 4px;
        width: 52px;
        height: 22px;
        display: inline-block;
        vertical-align: middle;
    }

    .skeleton-header {
        height: 20px;
        width: 100%;
    }

    .skeleton-box {
        height: 40px;
        width: 100%;
    }

    .skeleton-half-box {
        height: 40px;
        width: 48%;
        margin-right: 4%;
        display: inline-block;
    }

    @keyframes loading {
        0% {
            background-position: 200% 0;
        }

        100% {
            background-position: -200% 0;
        }
    }

    .classes-content {
        display: none;
    }

    .error-box {
        background-color: var(--submit-fail-background);
        color: var(--submit-fail-colour);
        padding: 10px 20px;
        /* border: 2px solid var(--submit-fail-colour); */
        border-radius: 10px;
        font-family: var(--normal-font);
        box-shadow: 0 0 0px rgba(0, 0, 0, 0.5);
        display: flex;
        align-items: center;
    }

    .error-box svg {
        margin-right: 10px;
    }

